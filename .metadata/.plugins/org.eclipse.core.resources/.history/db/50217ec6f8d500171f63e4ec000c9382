
public class UsingVariables 
{
	public static void main( String[] args)
	{
		int dank;
		double meme;
		String wtf;
		
		dank = 113;
		meme = 2.7182;
		wtf = Computer Science; 
		
		System.out.println( "This is room # " + dank );
		System.out.println( " e is close to " + meme );
		System.out.println( " I am learning a bit about " + wtf );
	}

}
